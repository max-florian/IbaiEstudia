{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    }
  }